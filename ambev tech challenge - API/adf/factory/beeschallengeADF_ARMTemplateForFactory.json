{
    "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "factoryName": {
            "type": "string",
            "metadata": "Data Factory name",
            "defaultValue": "beeschallengeADF"
        },
        "dataFactory_location": {
            "type": "string",
            "defaultValue": "eastus"
        },
        "dataFactory_identity_type": {
            "type": "string",
            "defaultValue": "SystemAssigned"
        }
    },
    "variables": {
        "factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
    },
    "resources": [
        {
            "name": "[parameters('factoryName')]",
            "type": "Microsoft.DataFactory/factories",
            "apiVersion": "2018-06-01",
            "properties": {},
            "dependsOn": [],
            "location": "[parameters('dataFactory_location')]",
            "identity": {
                "type": "[parameters('dataFactory_identity_type')]",
                "principalId": "b265512c-5781-4889-a49b-592824d9b514",
                "tenantId": "11dbbfe2-89b8-4549-be10-cec364e59551"
            }
        }
    ]
}